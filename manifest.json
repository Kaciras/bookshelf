{
	"manifest_version": 3,
	"name": "Bookshelf",
	"version": "1.9.0",
	"description": "Minimalist new tab page",
	"developer": {
		"name": "Kaciras",
		"url": "https://github.com/Kaciras/browser-theme"
	},
	"default_locale": "en",
	"icons": {
		"48": "./assets/Star.svg",
		"96": "./assets/Star.svg"
	},
	"applications": {
		"gecko": {
			"id": "theme@kaciras",
			"strict_min_version": "98.0"
		}
	},
	"permissions": [
		"topSites",
		"storage"
	],
	"host_permissions": ["*://*/*"],
	"chrome_url_overrides" : {
		"newtab": "./new-tab/index.html"
	}
}

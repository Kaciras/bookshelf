{
	"manifest_version": 2,
	"name": "Kaciras Firefox Theme",
	"version": "1.0.0",
	"description": "Kaciras Firefox Theme",
	"developer": {
		"name": "Kaciras",
		"url": "https://github.com/Kaciras/firefox-theme"
	},
	"applications": {
		"gecko": {
			"id": "theme@kaciras",
			"strict_min_version": "89.0"
		}
	},
	"permissions": [
		"storage",
		"<all_urls>"
	],
	"chrome_url_overrides" : {
		"newtab": "index.html"
	}
}